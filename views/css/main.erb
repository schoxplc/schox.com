.topbar div > ul, .nav {
  margin-top: 40px;
}

.topbar li {
  background-repeat   : no-repeat;
  background-position : top left;
  text-indent         : -4000px;
  width: 100px;
}

.topbar li.intro   { background-image: url(/images/nav/intro.png); }
.topbar li.founder { background-image: url(/images/nav/founder.png); }
.topbar li.team    { background-image: url(/images/nav/team.png); }
.topbar li.clients { background-image: url(/images/nav/clients.png); }
.topbar li.contact { background-image: url(/images/nav/contact.png); }
